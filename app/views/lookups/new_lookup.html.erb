<%-
  @title = 'Lookups'
  @left_column_partial = 'reg_admin/reg_admin_left_menu' 
-%>

<h1>New Lookup</h1>

<%= form_tag :controller => 'lookups', :action => 'create_lookup' do %>
  <%= render :partial => 'form_lookup' %>
  <%= submit_tag "Create" %>
<% end %>

<%= form_tag :controller => 'lookups', :action => 'index' do %> 
  <%= submit_tag "Cancel" %>
<% end %>
