<%-
  @title = 'Lookups'
  @left_column_partial = 'reg_admin/reg_admin_left_menu' 
-%>

<h1>Edit Lookup</h1>

<%= form_tag :controller => 'lookups', :action => 'update_lookup', :id => @lookup do %>
  <%= render :partial => 'form_lookup' %>
  <%= submit_tag 'Save' %>
<% end %>

<table>
	<tr>
		<td>
		<%= form_tag :controller => 'lookups', :action => 'index' do %> 
		  <%= submit_tag "Cancel" %>
		<% end %>
		</td>
		
		<td>
		<%= form_tag :controller => 'lookups', :action => 'destroy_lookup', :id => @lookup do %>
		  <%= submit_tag "Delete" %>
		  &nbsp;
		<% end %>
		</td>
	</tr>
</table>
