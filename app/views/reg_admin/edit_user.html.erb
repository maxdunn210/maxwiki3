<%
  @title = 'Users'
  @left_column_partial = 'reg_admin/reg_admin_left_menu' 
%>

<h1>Edit User</h1>

<%= form_tag :action => 'edit_user', :id => @user do %>
  <%= render_partial 'form' %>
  <br>
  <%= submit_tag 'Update' %>
<% end %>

<table>
<tr>
<td>
<%= form_tag :action => 'list_users' do %> 
  <%= submit_tag "Cancel" %>
<% end %>
</td>
<td>
<%= form_tag :action => 'delete_user', :id => @user do %> 
  <%= submit_tag "Delete" %>
<% end %>
</td>
</tr>
</table>